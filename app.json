{
  "expo": {
    "name": "web-radio-mobile",
    "slug": "web-radio-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-icons/mood_radio_ios_icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/app-icons/mood_radio_ios_icon.png",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ],
        "NSAppTransportSecurity": {
          "NSExceptionDomains": {
            "51.75.200.205": {
              "NSIncludesSubdomains": true,
              "NSExceptionAllowsInsecureHTTPLoads": true
            }
          }
        },
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.jonmourolin.web-radio-mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icons/mood_radio_android_icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "FOREGROUND_SERVICE",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "package": "com.jonmourolin.webradiomobile"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-splash-screen",
      [
        "expo-av",
        {
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "backgroundMode": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8463f286-642f-4244-a976-09dd4a641eec"
      }
    }
  }
}
